<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.myonlineapplicationmain">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UI.productCart"></activity>
        <activity android:name=".UI.Buy" />
        <activity android:name=".UI.MovieDetailActivity" />
        <activity android:name=".UI.SearchActivity" />
        <activity
            android:name=".UI.forget"
            android:noHistory="true" />
        <activity
            android:name=".UI.NewPassword"
            android:noHistory="true" />
        <activity
            android:name=".UI.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity
            android:name=".UI.LoginAct"
            android:noHistory="true" />
        <activity
            android:name=".UI.SignUpAct"
            android:noHistory="true" />
        <activity
            android:name=".UI.MainActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>